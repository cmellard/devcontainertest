{
  "build": {
    "dockerfile": "Dockerfile"
  },
  "postCreateCommand": "/workspaces/devcontainertest/.devcontainer/post-create-setup.sh",
  "containerEnv": {
    "DOCKER_HOST": "unix:///run/docker.sock",
    "PGDATABASE": "postgres",
    "GOPATH": "/go"
  },
  "remoteEnv": {
    "DATABASE_HOST": "/var/run/postgresql",
    "DATABASE_USER": "root",
    "DATABASE_NAME": "order",
    "RAILS_ENV": "test",
    "DYNO": "potato",
    "PROTOC_DISABLE_DOCKER": "true",
    "GOPRIVATE": "github.com/Shuttlerock",
    "PATH": "${remoteEnv:PATH}:/usr/local/go/bin:/go/bin"
  }
}
